{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "bun test",
      "program": "bun-debug",
      "args": ["wiptest", "${file}"],
      "cwd": "${workspaceFolder}",
      "env": {
        "FORCE_COLOR": "1"
      },
      "console": "internalConsole"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "bun test --watch",
      "program": "bun-debug",
      "args": ["--watch", "test", "${file}"],
      "cwd": "${workspaceFolder}",
      "env": {
        "FORCE_COLOR": "1"
      },
      "console": "internalConsole"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "bun test (all)",
      "program": "bun-debug",
      "args": ["wiptest"],
      "cwd": "${workspaceFolder}/test",
      "env": {
        "FORCE_COLOR": "1",
        "BUN_GARBAGE_COLLECTOR_LEVEL": "2"
      },
      "console": "internalConsole"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "bun run current file",
      "program": "bun-debug",
      "args": ["${file}"],
      "cwd": "${file}/../../",
      "env": {
        "FORCE_COLOR": "1"
      },
      "console": "internalConsole"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "bun run (watch)",
      "program": "bun-debug",
      "args": ["--watch", "${file}"],
      "cwd": "${file}/../../",
      "env": {
        "FORCE_COLOR": "1"
      },
      "console": "internalConsole"
    },
    {
      "type": "lldb",
      "request": "launch",
