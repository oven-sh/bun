    {
      "type": "lldb",
      "request": "launch",
      "name": "bun http example",
      "program": "bun-debug",
      "args": ["run", "examples/http.ts"],
      "cwd": "${workspaceFolder}",
      "env": {
        "FORCE_COLOR": "1"
      },
      "console": "internalConsole"
    },
    {
      "type": "lldb",
