{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "remix dev",
    "start": "remix-serve build",
    "typecheck": "tsc",
    "install:bun": "rm -rf node_modules && bun install",
    "install:pnpm": "rm -rf node_modules && pnpm install",
    "install:yarn": "rm -rf node_modules && yarn",
    "install:npm": "rm -rf node_modules && npm install",
    "bench": "hyperfine --warmup 2 'bun run install:bun' 'bun run install:pnpm' 'bun run install:yarn' 'bun run install:npm'"
  },
  "dependencies": {
    "@remix-run/node": "^1.15.0",
    "@remix-run/react": "^1.15.0",
    "@remix-run/serve": "^1.15.0",
    "isbot": "^3.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.15.0",
    "@remix-run/eslint-config": "^1.15.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "eslint": "^8.27.0",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
