{
  "name": "bun-otel",
  "version": "0.1.0",
  "type": "module",
  "module": "index.ts",
  "types": "index.ts",
  "description": "OpenTelemetry tracing and metrics bridge for Bun's native telemetry system - automatic distributed tracing and HTTP metrics for Bun.serve()",
  "exports": {
    ".": {
      "import": "./index.ts",
      "require": "./index.ts",
      "types": "./index.ts"
    },
    "./fetch-instrumentation": {
      "import": "./BunFetchInstrumentation.ts",
      "require": "./BunFetchInstrumentation.ts",
      "types": "./BunFetchInstrumentation.ts"
    },
    "./core": {
      "import": "./otel-core.ts",
      "require": "./otel-core.ts",
      "types": "./otel-core.ts"
    },
    "./types": {
      "import": "./otel-types.ts",
      "require": "./otel-types.ts",
      "types": "./otel-types.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "index.ts",
    "bun-sdk.ts",
    "BunFetchInstrumentation.ts",
    "BunServerMetricsInstrumentation.ts",
    "BunAsyncLocalStorageContextManager.ts",
    "otel-core.ts",
    "otel-types.ts",
    "tsconfig.json",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/oven-sh/bun",
    "directory": "packages/bun-otel"
  },
  "keywords": [
    "bun",
    "opentelemetry",
    "telemetry",
    "tracing",
    "metrics",
    "observability",
    "prometheus"
  ],
  "peerDependencies": {
    "@opentelemetry/api": "^1.0.0"
  },
  "dependencies": {
    "@opentelemetry/core": "^1.30.1",
    "@opentelemetry/resources": "^1.30.1",
    "@opentelemetry/sdk-metrics": "^1.30.1",
    "@opentelemetry/sdk-trace-base": "^1.30.1",
    "@opentelemetry/sdk-trace-node": "^1.30.1"
  },
  "devDependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/context-async-hooks": "^2.1.0",
    "@opentelemetry/instrumentation-fetch": "^0.206.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "bun-types": "canary"
  }
}
